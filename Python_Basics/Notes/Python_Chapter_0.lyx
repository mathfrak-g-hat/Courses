backgroundcolor "none"
status open

\begin_layout Plain Layout
ini_request = requests.get(
\color olive
"https://www.somewebsite.com"
\color inherit
)
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open


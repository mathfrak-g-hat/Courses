backgroundcolor "none"
status open

\begin_layout Plain Layout
ini_html= bs4.BeautifulSoup(ini_request.text,
\color olive
"lxml"
\color inherit
)
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
